# MHTAgentic Configuration
# Copy this file to .env and fill in your credentials

# Experity Credentials
EXPERITY_URL=https://app.experity.com
EXPERITY_USERNAME=your_username_here
EXPERITY_PASSWORD=your_password_here

# Browser Settings
# Set to true to run browser without visible window
HEADLESS=false
# Delay between browser actions in milliseconds (helps with stability)
SLOW_MO=100

# Output Settings
# Directory for exported files (relative to project root or absolute path)
OUTPUT_DIR=output
# Export format: "excel" or "csv"
EXPORT_FORMAT=excel

# Logging Settings
LOG_DIR=logs
LOG_LEVEL=INFO
